.school_title image{
    width: 100%;
    height: 144pt;
}
.school_address{
  height: 75pt;
  display: flex;
  justify-content: center;
}
.school_address_1{
  width: 90%;
  height: 75pt;
  background: #fff;
  z-index: 999;
  margin-top:-10pt; 
  border-radius:5px;
  border: 1px solid #F4F4F4;
  box-shadow:1px 1px 1px 1px #F4F4F4;
}
.school_address_1_1{
  padding: 5pt;
  font-size: 14pt;
  font-weight: bold;
}
.school_address_1_2{
  display: flex;
  flex-direction: row;
}
.school_address_1_2 image{
  padding: 5pt;
  height: 18pt;
  width: 18pt;
}
.content_title{
  display: flex;
  flex-direction: row;
  height: 34pt;
  padding-left: 3%;
  background: #F4F4F4;
  align-items: center;
}
.content_title_1{
   margin-top: 5pt;
}
.content_title_1 image{
  height: 14pt;
  width: 14pt;
}
.content_title_2{
  padding: 5pt;
  font-size: 14pt;
  font-weight: bold;
}
.content{
  padding-top:5pt; 
}
.content text{
 padding-left: 3%;
}
.content image{
  padding-top: 5pt;
  width: 100%;
}