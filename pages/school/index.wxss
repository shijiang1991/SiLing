.school_title image{
    width: 100%;
    height: 388rpx;
}
.school_address{
  height: 150rpx;
  display: flex;
  justify-content: center;
  width: 100%;
}
.school_address_1{
  width: 90%;
  height: 150rpx;
  background: #fff;
  z-index: 999;
  margin-top:-20rpx; 
  border-radius:10rpx;
  border: 1rpx solid #F4F4F4;
  box-shadow:2rpx 2rpx 2rpx 2rpx #F4F4F4;
}
.school_address_1_1{
  padding: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
}
.school_address_1_2{
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
}
.school_address_1_2 image{
  padding: 10rpx;
  height: 36rpx;
  width: 36rpx;
}
.content_title{
  display: flex;
  flex-direction: row;
  height: 68rpx;
  padding-left: 3%;
  background: #F4F4F4;
  align-items: center;
}
.content_title_1{
   margin-top: 10rpx;
}
.content_title_1 image{
  height: 28rpx;
  width: 28rpx;
}
.content_title_2{
  padding: 10rpx;
  font-size: 28rpx;
  font-weight: bold;
}
.content{
  width: 94%;
  padding:3%; 
  font-size: 28rpx;
}